  "version": "0.2.0",
  "configurations": [
    {
      "name": "Init",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/arbor",
      "args": [
        "init"
      ],
      "env": {},
      "buildFlags": "",
      "dlvFlags": [
        "--check-go-version=false"
      ]
    },
    {
      "name": "Add",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/arbor",
      "args": [
        "add",
        "demo.txt"
      ],
      "env": {},
      "buildFlags": "",
      "dlvFlags": [
        "--check-go-version=false"
      ]
    },
    {
      "name": "Commit",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/arbor",
      "args": [
        "commit",
        "-m",
        "my test commit"
      ],
      "env": {},
      "buildFlags": "",
      "dlvFlags": [
        "--check-go-version=false"
      ]
    },
    {
      "name": "Logs",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/arbor",
      "args": [
        "log"
      ],
      "env": {},
      "buildFlags": "",
      "dlvFlags": [
        "--check-go-version=false"
      ]
    }
  ]
}